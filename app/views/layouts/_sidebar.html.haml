
- if current_user
  .card
    .card-header
      = current_user.username
    %ul
      %li= link_to 'Inicio', about_index_path
      -if current_user.teacher?
        %li= link_to 'Alumnos', students_path
        %li= link_to 'Lecturas', levels_path
      -else
        %li Leccion 1
        %li Leccion 2
        %li Leccion 3
        %li Leccion 4
